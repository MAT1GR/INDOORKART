{
  "name": "rosario-indoor-kart",
  "version": "1.0.0",
  "description": "Sistema completo de reservas para Rosario Indoor Kart",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:full": "concurrently \"npm run dev:server -- --host\" \"npm run dev -- --host\"",
    "build": "tsc && vite build",
    "build:server": "tsc server/index.ts --outDir dist",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "prisma generate",
    "db:migrate": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed"
  },
  "keywords": [
    "karting",
    "reservas",
    "react",
    "nodejs",
    "prisma",
    "tailwindcss"
  ],
  "author": "Rosario Indoor Kart",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "nodemailer": "^7.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
